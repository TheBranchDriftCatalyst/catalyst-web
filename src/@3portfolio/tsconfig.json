{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
    },
    "types": [
      "@modyfi/vite-plugin-yaml/modules"
    ],
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
, "../components/Experience.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
